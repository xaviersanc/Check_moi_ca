{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/steamspy",
      "dest": "/api/steamspy.js"
    },
    {
      "src": "/api/steamstore",
      "dest": "/api/steamstore.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
